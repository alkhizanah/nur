benchmarker = import("benchmarker.nur")

benchmarker.run("common arithmetic operations loop", fn () {
    result = 1

    i = 1

    while i < 10 ** 6 + 1 {
        result %= i
        result += i
        result -= i
        result /= i
        result *= i
        result **= i

        i += 1
    }
})
