benchmarker = import("benchmarker.sy")

benchmarker.run(fn () {
    cache = {}

    fib = fn (n) {
        if n < 2 {
            return n
        }

        if contains(map_keys(cache), n) {
            return cache[n]
        }

        result = fib(n - 1) + fib(n - 2)

        cache[n] = result

        return result
    }

    println(fib(30))
})
