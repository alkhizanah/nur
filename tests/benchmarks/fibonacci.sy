benchmarker = import("benchmarker.sy")

benchmarker.run("fibonacci recursive function (cached)", fn () {
    cache = {}

    fib = fn (n) {
        if n < 2 {
            return n
        }

        if contains(cache, n) {
            return cache[n]
        }

        result = fib(n - 1) + fib(n - 2)

        cache[n] = result

        return result
    }

    println(fib(30))
})

benchmarker.run("fibonacci recursive function (not cached)", fn () {
    fib = fn (n) {
        if n < 2 {
            return n
        }

        return fib(n - 1) + fib(n - 2)
    }

    println(fib(30))
})
