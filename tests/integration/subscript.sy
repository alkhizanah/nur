tester = import("tester.sy")

tester.run("subscript an array", fn () {
    arr = [0, 2, 4, 3, 5]

    return arr[3] == 3
})

tester.run("subscript a string", fn () {
    str = "Hello, World!"

    return str[3] == "l"
})

tester.run("subscript a map", fn () {
    map = {1: 2, 3: 4, 4: 5}

    return map[3] == 4
})

tester.end()
