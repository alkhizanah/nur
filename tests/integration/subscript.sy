tester = import("tester.sy")

test_1 = fn () {
    # subscript an array
    arr = [0, 2, 4, 3, 5]

    return arr[3] == 3
}

test_2 = fn () {
    # subscript a string
    str = "Hello, World!"

    return str[3] == "l"
}

test_3 = fn () {
    # subscript a map
    map = {1: 2, 3: 4, 4: 5}

    return map[3] == 4
}

tester.test([
    test_1,
    test_2,
    test_3,
])
